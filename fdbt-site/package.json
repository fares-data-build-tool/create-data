        "@types/micro": "^7.3.3",
        "@types/next": "^9.0.0",
        "@types/nookies": "^2.0.3",
        "@types/uuid": "^3.4.6",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-sass": "^1.0.1",
        "aws-event-mocks": "0.0.0",
        "axios": "^0.19.0",
        "base-64": "^0.1.0",
        "cookies": "^0.8.0",
        "enzyme-to-json": "^3.4.3",
        "formidable": "^1.2.1",
        "govuk-frontend": "^3.4.0",
        "micro": "^9.3.5-canary.3",
        "next": "^9.2.1",
        "next-compose-plugins": "^2.2.0",
        "next-cookie": "^2.0.0",
        "next-cookies": "^2.0.3",
        "next-fonts": "^0.19.0",
        "next-images": "^1.2.0",
        "node-sass": "^4.13.0",
        "nookies": "^2.0.8",
        "react": "^16.12.0",
