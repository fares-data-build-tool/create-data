{
    "name": "site",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test": "jest --runInBand",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --watchAll",
        "test:ci": "CI=true jest --silent -w 1 --forceExit",
        "test:updateSnapshots": "jest --updateSnapshot",
        "dev": "export NODE_ENV='development' && ts-node --project tsconfig.server.json server/index.ts",
        "build:next": "next build",
        "build:server": "tsc --project tsconfig.server.json",
        "build": "npm run build:next && npm run build:server",
        "start": "NODE_ENV=production PORT=80 node dist/index.js",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint --fix . --ext .ts,.tsx"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "NODE_ENV=production lint-staged && npm run test:ci && tsc"
        }
    },
    "lint-staged": {
        "./**/*.{ts,tsx}": [
            "eslint --fix . --ext .ts,.tsx",
            "prettier --c './**/*.{ts,tsx}'"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-amplify/auth": "^3.2.9",
        "aws-event-mocks": "0.0.0",
        "aws-param-store": "^3.2.0",
        "aws-sdk": "^2.675.0",
        "axios": "^0.19.2",
        "base-64": "^0.1.0",
        "cookies": "^0.8.0",
        "csv-parse": "^4.9.1",
        "dateformat": "^3.0.3",
        "enzyme-to-json": "^3.4.4",
        "express": "^4.17.1",
        "formidable": "^1.2.2",
        "govuk-frontend": "^3.6.0",
        "helmet": "^3.22.0",
        "mock-express-request": "^0.2.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "mysql2": "^2.1.0",
        "next": "^9.4.2",
        "next-compose-plugins": "^2.2.0",
        "next-images": "^1.3.1",
        "nocache": "^2.1.0",
        "nookies": "^2.3.0",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "uuid": "^7.0.2",
        "xml2js": "^0.4.23",
        "yup": "^0.29.0"
    },
