        "@types/cookies": "^0.7.4",
        "@types/dateformat": "^3.0.1",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/formidable": "^1.0.31",
        "@types/helmet": "0.0.47",
        "@types/jest": "^25.1.4",
        "@types/micro": "^7.3.3",
        "@types/mock-req-res": "^1.1.0",
        "@types/morgan": "^1.9.0",
        "@types/moxios": "^0.4.9",
        "@types/multer": "^1.4.2",
        "@types/mysql2": "github:ljones92/mysql2",
        "@types/next": "^9.0.0",
        "@types/node": "^13.13.6",
