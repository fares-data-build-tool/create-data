        "@types/jest": "^24.0.24",
        "@types/mock-req-res": "^1.1.0",
        "@types/moxios": "^0.4.9",
        "@types/node": "^12.12.18",
        "@types/react": "^16.9.16",
        "@types/sinon": "^7.5.1",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "aws-lambda-mock-context": "^3.2.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
