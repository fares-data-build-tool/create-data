<?xml version="1.0" encoding="utf-8" ?>
<!-- NEED TO CLARIFY WITH NICK KNOWLES WHERE THE PublicationDelivery TAGS COME FROM -->
<PublicationDelivery version="1.1" xsi:schemaLocation="http://netex.uk/netex/schema.htm http://netex.uk/netex/schema/1.09c/xsd/NeTEx_publication.xsd" xmlns="http://netex.uk/netex" xmlns:siri="http://www.transmodel-cen.eu/standards/siri/" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/>
    <Description>Example NeTEx for simple point-to-point single fares</Description>
    <dataObjects>
        <CompositeFrame version="1.0" id="mb:Metrobus@Products@Trip@Line_1" dataSourceRef="mb:metrobus" responsibilitySetRef="mb:tariffs">
			<ValidBetween>
				<FromDate>2017-01-01T00:00:00</FromDate>
				<ToDate>2020-12-31T12:00:00</ToDate>
			</ValidBetween>
			<Name>MetroBus 1 0 - Fares Example</Name>
			<Description>Point to point fare</Description>
			<TypeOfFrameRef ref="fxc:UK_Bus_tariff_profile@composite" version="fxc:v1.0"/>
			<!--==== CODESPACEs ==== -->
			<codespaces>
				<CodespaceRef ref="napt"/>
				<CodespaceRef ref="naptStop"/>
				<CodespaceRef ref="nptgUkLocality"/>
				<CodespaceRef ref="nptgUkAdministrativeArea"/>
				<CodespaceRef ref="noc"/>
				<CodespaceRef ref="mb"/>
			</codespaces>
			<!--==== FRAME DEFAULTS ==== -->
			<FrameDefaults>
				<!---  The default means that actually we could leave off all ids that belong to the mb: namespace. -->
				<DefaultCodespaceRef ref="mb"/>
				<DefaultDataSourceRef ref="mb:metrobus" version="1.0"/>
				<DefaultCurrency>GBP</DefaultCurrency>
            </FrameDefaults>
			<frames>
				
				<!--==== STEP 1:: Define some data regarding the operator - OPERATOR COMMON RESOURCES ====-->
				<ResourceFrame version="1.0" id="mb:Metrobus" dataSourceRef="mb:metrobus" responsibilitySetRef="mb:network_data">
					<Name>Opeartor specific  common resources</Name>
					<TypeOfFrameRef ref="fxc:UK_Bus_tariff_profile@operator_resources" version="fxc:v1.0"/>
					<codespaces>
						<Codespace id="mb">
							<Xmlns>mb</Xmlns>
							<XmlnsUrl>#################</XmlnsUrl>
							<Description>Metrobus data</Description>
						</Codespace>
					</codespaces>
					<dataSources>
						<DataSource id="mb:metrobus" version="1.0">
							<Email>feedback@metrobus.co.uk</Email>
						</DataSource>
					</dataSources>
					<!-- ========Responsibility Sets========== -->
					<responsibilitySets>
						<ResponsibilitySet version="1.0" id="mb:tariffs">
							<Name>Opeartor Tariffs</Name>
							<roles>
								<ResponsibilityRoleAssignment version="1.0" id="mb:tariff_data@creates">
									<DataRoleType>creates</DataRoleType>
									<StakeholderRoleType>FareManagement</StakeholderRoleType>
									<ResponsibleOrganisationRef ref="noc:137122" version="1.0">Metrobus</ResponsibleOrganisationRef>
								</ResponsibilityRoleAssignment>
							</roles>
						</ResponsibilitySet>
						<ResponsibilitySet version="1.0" id="mb:network_data">
							<Name>Operator data</Name>
							<roles>
								<ResponsibilityRoleAssignment version="1.0" id="mb:network_data@creates">
									<DataRoleType>creates</DataRoleType>
									<StakeholderRoleType>Planning</StakeholderRoleType>
										<ResponsibleOrganisationRef ref="noc:137122" version="1.0">Metrobus</ResponsibleOrganisationRef>
								</ResponsibilityRoleAssignment>
							</roles>
						</ResponsibilitySet>
					</responsibilitySets>
					<!--==== CODE VALUES  ==== -->
					<typesOfValue>
						<ValueSet version="1.0" id="mb:Branding" classOfValues="Branding">
							<Name>Operator Branding</Name>
							<values>
								<Branding version="1.0" id="mb:Metrobus@brand">
									<Name>Metrobus</Name>
									<Url>https://www.metrobus.co.uk/static/images/colorways/metrobus/logo.png</Url>
								</Branding>
							</values>
						</ValueSet>
					</typesOfValue>
					<organisations>
						<!-- ==== ORGANISATIONS ==== -->
						<Operator version="1.0" id="noc:137122">
							<PublicCode>METR</PublicCode>
							<Name>Metrobus</Name>
							<ShortName>Metrobus</ShortName>
							<TradingName>Metrobus Ltd</TradingName>
							<ContactDetails>
								<Phone>01293 449191</Phone>
							</ContactDetails>
							<OrganisationType>operator</OrganisationType>
							<Address>
								<Street>Wheatstone Close</Street>
								<Town>Crawley</Town>
								<PostCode>RH10 9UA</PostCode>
								<PostalRegion>West Sussex</PostalRegion>
							</Address>
							<PrimaryMode>bus</PrimaryMode>
						</Operator>
					</organisations>
                </ResourceFrame>
    </dataObjects>