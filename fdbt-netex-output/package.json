{
    "name": "netex-output",
    "version": "1.0.0",
    "scripts": {
        "build": "tsc --noEmit",
        "test": "jest --runInBand",
        "test:ci": "CI=true jest --testRegex src/**/*.test.ts -w 1",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "NODE_ENV=production lint-staged && npm run test:ci && npx audit-ci --moderate"
        }
    },
    "lint-staged": {
        "*.{js,jsx,.ts,.tsx}": [
            "pretty-quick --staged",
            "eslint --fix"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/aws-lambda": "^8.10.36",
        "@types/node": "^12.12.14",
        "@types/nodemailer": "^6.4.0",
        "@types/xml-formatter": "^1.1.0",
        "@types/xml2json": "^0.11.0",
        "aws-lambda": "^1.0.5",
        "aws-sdk": "^2.610.0",
        "nodemailer": "^6.4.4",
        "typescript": "^3.7.3",
        "xml-formatter": "^1.2.0",
        "xml2json": "^0.12.0"
    },
    "devDependencies": {
        "@types/aws-lambda-mock-context": "^3.2.0",
        "@types/jest": "^24.0.23",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "aws-lambda-mock-context": "^3.2.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.2.1",
        "jest": "^24.9.0",
        "lint-staged": "^10.0.7",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "serverless-dynamodb-local": "^0.2.38",
        "serverless-offline": "^5.12.1",
        "serverless-plugin-typescript": "^1.1.9",
        "serverless-s3-local": "^0.5.3",
        "ts-jest": "^24.3.0"
    }
}
