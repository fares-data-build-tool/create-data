{
    "name": "netex-output",
    "version": "1.0.0",
    "scripts": {
        "build": "tsc",
        "test": "jest --runInBand",
        "test:ci": "CI=true jest --findRelatedTests src/netex-emailer/handler.test.ts src/netex-convertor/* src/data/auroradb.test.ts --maxWorkers=1",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "netexConvert": "NODE_ENV=development UNVALIDATED_NETEX_BUCKET=$UNVALIDATED_NETEX_BUCKET ts-node src/netex-convertor/run-local.ts \"$EVENT_DATA\"",
        "netexEmail": "NODE_ENV=development MATCHING_DATA_BUCKET=$MATCHING_DATA_BUCKET ts-node src/netex-emailer/run-local.ts \"$EVENT_DATA\"",
        "precommit": ". NODE_ENV=production lint-staged && npm run test:ci",
        "prepare": "cd ../.. && husky install repos/fdbt-netex-output/.husky",
        "tsc": "./node_modules/.bin/tsc && ./node_modules/.bin/tsc --project tsconfig.json"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "eslint --fix . --ext .ts,.tsx"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.705.0",
        "@aws-sdk/client-ses": "^3.699.0",
        "@aws-sdk/client-sns": "^3.699.0",
        "@aws-sdk/client-ssm": "^3.699.0",
        "aws-lambda": "^1.0.7",
        "fdbt-types": "file:../fdbt-types",
        "lodash": "^4.17.21",
        "moment": "^2.27.0",
        "moment-timezone": "^0.5.31",
        "mysql2": "^3.11.5",
        "nodemailer": "^6.6.2",
        "typescript": "4.3.5",
        "uuid": "^8.3.2",
        "xml2json": "^0.12.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.146",
        "@types/aws-lambda-mock-context": "^3.2.4",
        "@types/aws-param-store": "^2.1.3",
        "@types/jest": "^27.0.2",
        "@types/libxslt": "^0.9.0",
        "@types/lodash": "^4.14.170",
        "@types/node": "^18.19.19",
        "@types/nodemailer": "^6.4.2",
        "@types/uuid": "^8.3.4",
        "@types/xml2json": "^0.11.0",
        "@typescript-eslint/eslint-plugin": "4.27.0",
        "@typescript-eslint/parser": "4.31.2",
        "aws-lambda-mock-context": "^3.2.1",
        "eslint": "7.32.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^5.0.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^8.0.0",
        "jest": "^26.6.3",
        "libxslt": "^0.10.1",
        "lint-staged": "^10.0.8",
        "prettier": "^1.19.1",
        "serverless-dynamodb-local": "^1.0.2",
        "serverless-plugin-log-retention": "^2.0.0",
        "serverless-plugin-typescript": "^2.0.0",
        "serverless-python-requirements": "^5.1.0",
        "ts-jest": "^26.4.4",
        "ts-node": "^10.2.1"
    }
}
