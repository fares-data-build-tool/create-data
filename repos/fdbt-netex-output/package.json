{
    "name": "netex-output",
    "version": "1.0.0",
    "scripts": {
        "build": "tsc --noEmit",
        "test": "jest --runInBand",
        "test:ci": "CI=true jest --maxWorkers=1",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "netexConvert": "NODE_ENV=development UNVALIDATED_NETEX_BUCKET=$UNVALIDATED_NETEX_BUCKET ts-node src/netex-convertor/run-local.ts \"$EVENT_DATA\"",
        "netexEmail": "NODE_ENV=development MATCHING_DATA_BUCKET=$MATCHING_DATA_BUCKET ts-node src/netex-emailer/run-local.ts \"$EVENT_DATA\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "NODE_ENV=production lint-staged && npm run test:ci && npx audit-ci --moderate"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "eslint --fix . --ext .ts,.tsx",
            "pretty-quick --staged"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "aws-lambda": "^1.0.5",
        "aws-param-store": "^3.2.0",
        "aws-sdk": "^2.639.0",
        "libxslt": "^0.9.0",
        "lodash": "^4.17.21",
        "moment": "^2.27.0",
        "moment-timezone": "^0.5.31",
        "mysql2": "^2.1.0",
        "nodemailer": "^6.4.5",
        "typescript": "^3.8.3",
        "xml2json": "^0.12.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.46",
        "@types/aws-lambda-mock-context": "^3.2.0",
        "@types/aws-param-store": "^2.1.1",
        "@types/jest": "^24.0.23",
        "@types/libxslt": "^0.9.0",
        "@types/lodash": "^4.14.155",
        "@types/node": "^12.12.30",
        "@types/nodemailer": "^6.4.0",
        "@types/xml2json": "^0.11.0",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "aws-lambda-mock-context": "^3.2.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.2.1",
        "jest": "^26.6.3",
        "lint-staged": "^10.0.8",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "serverless-dynamodb-local": "^0.2.39",
        "serverless-offline": "^6.5.0",
        "serverless-plugin-typescript": "^1.1.9",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1"
    }
}
