        "browserstack-cypress-cli": "^1.25.3",
        "cypress": "13.2.0",
        "cypress-file-upload": "^5.0.5",
        "eslint": "^8.49.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.0.0",
