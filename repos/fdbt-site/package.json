{
    "name": "site",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test": "jest",
        "tsc": "./node_modules/.bin/tsc && ./node_modules/.bin/tsc --project tsconfig.server.json",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --watchAll",
        "test:ci": "CI=true jest --silent -w 1 --forceExit",
        "test:updateSnapshots": "jest --updateSnapshot",
        "dev": "export NODE_ENV='development' && ts-node --project tsconfig.server.json server/index.ts",
        "build:next": "next build",
        "build:server": "tsc --project tsconfig.server.json",
        "build": "npm run build:next && npm run build:server",
        "start": "NODE_ENV=production PORT=80 node dist/server/index.js",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint --fix . --ext .ts,.tsx",
        "lint:cypress": "cd cypress_tests && npm run lint",
        "lint:cypress-fix": "cd cypress_tests && npm run lint:fix",
        "postshrinkwrap": "npm install --no-package-lock --no-audit --no-fund --no-progress --silent && npx prettier --write ./package-lock.json",
        "precommit": ". npm run lint && npm run lint:cypress && npm run tsc && CI=true jest --silent",
        "prepare": "cd ../.. && husky install repos/fdbt-site/.husky"
    },
    "lint-staged": {
        "./**/*.{ts,tsx}": [
            "prettier --c './**/*.{ts,tsx}'"
        ],
        "./**/*.{ts,tsx},.eslintignore": [
            "npm run lint:fix"
        ],
        "./cypress_tests/**/*.{ts,tsx}": [
            "npm run lint:cypress"
        ]
    },
