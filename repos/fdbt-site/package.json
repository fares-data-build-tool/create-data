{
    "name": "site",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --watchAll",
        "test:ci": "CI=true jest --silent -w 1 --forceExit",
        "test:updateSnapshots": "jest --updateSnapshot",
        "dev": "export NODE_ENV='development' NODE_OPTIONS='--inspect' && ts-node --project tsconfig.server.json server/index.ts",
        "build:next": "next build",
        "build:server": "tsc --project tsconfig.server.json",
        "build": "npm run build:next && npm run build:server",
        "start": "NODE_ENV=production PORT=80 node dist/server/index.js",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint --fix . --ext .ts,.tsx",
        "lint:cypress": "cd cypress_tests && npm run lint:fix",
        "postshrinkwrap": "npx --quiet npm-force-resolutions && npm install --no-package-lock --no-audit --no-fund --no-progress --silent && npx prettier --write ./package-lock.json"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": ". ${NVM_DIR}/nvm.sh && nvm use && NODE_ENV=production lint-staged && CI=true jest --silent && tsc && tsc --project tsconfig.server.json && tsc --project cypress_tests"
        }
    },
    "lint-staged": {
        "./**/*.{ts,tsx}": [
            "prettier --c './**/*.{ts,tsx}'"
        ],
        "./**/*.{ts,tsx},.eslintignore": [
            "npm run lint:fix"
        ],
        "./cypress_tests/**/*.{ts,tsx}": [
            "npm run lint:cypress"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "aws-param-store": "^3.2.0",
