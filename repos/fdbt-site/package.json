{
    "name": "site",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test": "jest --runInBand",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --watchAll",
        "test:ci": "CI=true jest --silent -w 1 --forceExit",
        "test:updateSnapshots": "jest --updateSnapshot",
        "dev": "export NODE_ENV='' && next -p 5555",
        "build": "NODE_ENV=production next build",
        "start": "next start -p 80",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint --fix . --ext .ts,.tsx"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "NODE_ENV=production lint-staged && npm run test:ci && tsc"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "eslint --fix . --ext .ts,.tsx",
            "pretty-quick --staged"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "array.prototype.flatmap": "^1.2.3",
        "aws-event-mocks": "0.0.0",
        "aws-sdk": "^2.636.0",
        "axios": "^0.19.2",
        "base-64": "^0.1.0",
        "cookies": "^0.8.0",
        "csv-parse": "^4.8.8",
        "dateformat": "^3.0.3",
        "enzyme-to-json": "^3.4.4",
        "formidable": "^1.2.2",
        "govuk-frontend": "^3.6.0",
        "mock-express-request": "^0.2.2",
        "multer": "^1.4.2",
        "next": "^9.3.0",
        "next-compose-plugins": "^2.2.0",
        "next-fonts": "^1.0.3",
        "next-images": "^1.3.1",
        "nookies": "^2.1.1",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "uuid": "^7.0.2",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@types/array.prototype.flatmap": "^1.2.1",
        "@types/aws-lambda": "^8.10.46",
        "@types/aws-lambda-mock-context": "^3.2.0",
        "@types/axios": "^0.14.0",
        "@types/cookies": "^0.7.4",
        "@types/dateformat": "^3.0.1",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/formidable": "^1.0.31",
        "@types/jest": "^25.1.4",
        "@types/micro": "^7.3.3",
        "@types/mock-req-res": "^1.1.0",
        "@types/moxios": "^0.4.9",
        "@types/multer": "^1.4.2",
        "@types/next": "^9.0.0",
        "@types/node": "^13.9.0",
        "@types/nookies": "^2.0.3",
        "@types/react": "^16.9.23",
        "@types/sinon": "^7.5.2",
        "@types/uuid": "^7.0.0",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "aws-lambda-mock-context": "^3.2.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
